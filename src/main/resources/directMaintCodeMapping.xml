<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
  <stream name="directMaintCodeMappingFile" format="csv">
    <record name="maintCodeMappingVO" class="com.mikealbert.data.vo.MaintCodeMappingVO">
      <field name="mafsMaintCode" maxLength="10" required="true" trim="true" />
      <field name="vendorMaintCode" maxLength="25" required="true" trim="true" />
      <field name="vendorDescription" maxLength="80" required="true" trim="true" />
    </record>
  </stream>
</beanio>